    "eslint-plugin-import": "^2.10.0",
    "jest": "^22.4.3",
    "jest-raw-loader": "^1.0.1",
    "regenerator-runtime": "^0.11.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  },
  "dependencies": {},
